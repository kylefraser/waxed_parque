doctype html
html

  head
    meta(charset='utf8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
    meta(name='description', content='description of your site')
    meta(name='author', content= "author of the site")
    meta(name="viewport", content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no")
    title= _path
    link(href="https://fonts.googleapis.com/css?family=Montserrat|Oswald" rel="stylesheet")
    link(href="css/slick.css" rel="stylesheet")
    != css()

  body
    block content

    != js()
    script(src="http://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.15/angular.min.js")
    script(src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js")
    script(src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js")
    script(data-main="js/main", src="js/plangular.js")
    script(data-main="js/main", src="js/soundcloud.js")
    script(data-main="js/main", src="js/slick.min.js")
    script(data-main="js/main", src="js/main.js")
    
